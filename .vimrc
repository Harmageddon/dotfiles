highlight PreProc term=underline ctermfg=6
highlight Constant ctermfg=1
highlight LineNr ctermfg=DarkGray

set tabstop=2
set shiftwidth=2
set expandtab
set autoindent
set cindent
set number
set mouse=a
syntax on
filetype indent plugin on

au BufNewFile,BufFilePre,BufRead *.md set filetype=markdown

